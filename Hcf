package PRACTICE6;

import java.util.Scanner;

public class Hcf {
    public static int m1(int n, int m) {
        int r = 0;
        for(int i =1;i<=n||i<=m;i++ ){
            if(n%i==0 && m%i ==0){
                r =i;
            }
        }
        return r;
}   public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n1 = sc.nextInt();
        int n2 = sc.nextInt();
        System.out.println(m1(n1,n2));
    }

    }

